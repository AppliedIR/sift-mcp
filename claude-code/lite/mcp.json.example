{
  "mcpServers": {
    "forensic-rag": {
      "command": "__VENV__/bin/python",
      "args": ["-m", "rag_mcp.server"],
      "env": {
        "PYTHONPATH": "__SRC__/packages/forensic-rag/src",
        "RAG_INDEX_DIR": "__INDEX_DIR__",
        "AIIR_CASE_DIR": "__CASE_DIR__",
        "ANONYMIZED_TELEMETRY": "False"
      }
    },
    "windows-triage": {
      "command": "__VENV__/bin/python",
      "args": ["-m", "windows_triage.server"],
      "env": {
        "PYTHONPATH": "__SRC__/packages/windows-triage/src",
        "WT_DATA_DIR": "__DB_DIR__",
        "AIIR_CASE_DIR": "__CASE_DIR__"
      }
    }
  }
}
